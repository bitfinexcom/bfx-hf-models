{
  "name": "bfx-hf-models",
  "version": "2.1.5",
  "description": "HF DB models",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint lib/ examples/ test/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "rm -rf db/test.json && NODE_PATH=lib/ DB_FILENAME=./db/test.json NODE_ENV=test nyc --reporter=lcov --reporter=html mocha -b --recursive && rm -f db/test.json",
    "docs": "rm -rf docs/*.md && ./node_modules/.bin/jsdoc2md -f index.js 'lib/**/*.js' > docs/reference.md"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-models.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-models/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "bfx-api-node-rest": "^3.0.16",
    "bfx-api-node-util": "^1.0.2",
    "bfx-hf-util": "^1.0.1",
    "binary-search-insert": "^1.0.3",
    "bluebird": "^3.5.1",
    "copy": "^0.3.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "p-iteration": "^1.1.8",
    "promise-throttle": "^1.0.0"
  },
  "devDependencies": {
    "bfx-hf-ext-plugin-bitfinex": "^1.0.0",
    "bfx-hf-models-adapter-lowdb": "^1.0.0",
    "bfx-hf-models-adapter-sql": "^1.0.0",
    "chai": "^4.2.0",
    "create-index": "^2.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^27.0.4",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0"
  }
}
